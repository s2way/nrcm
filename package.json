{
  "bugs": {
    "url": "http://www.s2way.com/nrcm/issues",
    "email": "support@s2way.com"
  },
  "description": "The official S2Way Framework for Node.js.",
  "engines": {
    "node": ">=0.8.8"
  },
  "homepage": "http://www.s2way.com/nrcm",
  "keywords": [
    "s2way",
    "framework",
    "nrcm",
    "request",
    "view",
    "controller"
  ],
  "main": "./src/NRCM.js",
  "license": "Apache2",
  "name": "nrcm",
  "dependencies": {
    "chalk": "^0.5.1",
    "couchbase": "^1.2.4",
    "mysql": "^2.4.2",
    "winston": "^0.7.3"
  },
  "devDependencies": {
    "blanket": "^1.1.6",
    "grunt": "^0.4.5",
    "grunt-contrib-watch": "^0.6.1",
    "grunt-exec": "^0.4.6",
    "grunt-mocha-test": "^0.11.0",
    "jslint": "^0.5.2",
    "karma": "^0.12.21",
    "karma-coverage": "^0.2.6",
    "karma-mocha": "^0.1.7",
    "load-grunt-tasks": "^0.6.0",
    "mocha": "1.13.0",
    "travis-cov": "^0.2.5"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/nrcm/nrcm.git"
  },
  "version": "0.1.3",
  "scripts": {
    "preinstall": "npm install -g grunt-cli jslint mocha blanket",
    "pretest": "jslint --node --vars --devel --nomen --stupid --indent 4 --maxlen 2048 src/*.js test/*.js src/**/*.js test/**/*.js",
    "test": "mocha test/** -R spec && mocha test/** -r blanket -R html-cov > report.html",
    "posttest": "mocha test/** -r blanket -R travis-cov"
  },
  "contributors": [
    {
      "name": "Juliano Lazzarotto",
      "email": "jl.br.nh@gmail.com"
    },
    {
      "name": "Davi Gabriel da Silva",
      "email": "davi.gbr@gmail.com"
    }
  ],
  "readme": "Please reade the README.md file.",
  "readmeFilename": "README.md",
  "_id": "nrcm@0.1.1",
  "_from": "nrcm@",
  "config": {
    "blanket": {
      "pattern": "nrcm/src",
      "data-cover-never": "node_modules"
    },
    "travis-cov": {
      "threshold": 90,
      "removeKey": "branchFcn"
    }
  }
}
